    <main>
        <div class="user-signin">

            <div class="user-signin-titleContainer">
                <h1 class="user-signin-title">Já sou cadastrado</h1>
            </div>

            <form class="signin-form">

                <div class="MuiFormControll-root">
                    <input type="email" class="signinEmail" placeholder="E-mail" autocomplete="off">
                </div>
                
                <div class="MuiFormControll-root">
                    <input type="password" class="signinPassword" placeholder="Senha" autocomplete="off">
                </div>

                <div class="MuiGrid-item">
                    <a href="" class="MuiTypography-body MuiTypography-colorPrimary">Esqueci meu login ?</a>
                </div>
                
                <button type="submit" class="signin-btn">Acessar</button>

            </form>
            
        </div>

        <div class="user-signup">
            <h1 class="user-signup-title">Não sou cadastrado</h1>

            <div class="order-signup-list">

                <ul class="list-inline">
                    <li>100% seguro</li>
                    <li>100% confiável</li>
                </ul>

            </div>

            <a href="/cadastro" class="btnSignup-btn"><button type="submit" class="signup-btn">Cadastre-se</button></a>

        </div>

